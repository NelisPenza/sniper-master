{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "script",
    "ecmaFeatures": {
      "modules": false
    }
  },
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "no-plusplus": "off"
  },
  "ignorePatterns": [
    "*.test.js",
    "*.html"
  ]
}
